{
    "name":"Hello Extensions",
    "description":"Base Level Extension",
    "version":"1.0",
    "manifest_version":2,
    "browser_action":{
      "default_popup":"hello.html",
      "default_icon":"icons/16.png"
    },
    "icons": {
      "16": "icons/16.png",
      "48":"icons/48.png",
      "128":"icons/128.png"
    },
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+F",
          "mac": "MacCtrl+Shift+F"
        },
        "description": "Opens hello.html"
      }
    }
  }
  